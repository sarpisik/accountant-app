{
  "version": 2,
  "name": "nextjs-express-nowv2",
  "builds": [
    {
      "src": "app/routes/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "www/package.json",
      "use": "@now/next"
    }
  ],
  "public": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/routes/api"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/www/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "@my-mongodb-uri",
    "TOKEN_KEY": "@token-key",
    "EMAIL_HOST": "@email-host",
    "EMAIL_AUTH_USER": "@email-auth-user",
    "EMAIL_AUTH_PASSWORD": "@email-auth-password",
    "EMAIL_SENDER": "@email-sender"
  }
}
